from modules.utils import md5
from flask import jsonify

_games = {}

def get_games():
    return jsonify(_games), 200

def put_game(name, password):
    game = {"password":md5(password)}
    _games[name] = game
    return jsonify(game)

def get_game(name):
    if name in _games.keys():
        return jsonify(_games[name]), 200
    return "There isn't such game", 404
